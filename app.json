{
  "expo": {
    "name": "Mikello-App",
    "slug": "Mikello-App",
    "version": "3.0.0",
    "orientation": "portrait",
    "icon": "./assets/profile-pic.png",
    "splash": {
      "image": "./assets/profile-pic.png",
      "resizeMode": "contain",
      "backgroundColor": "#cee6f6"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/profile-pic.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "login.app.wercia.com",
      "versionCode": 1,
      "permissions": [
        "READ_CALENDAR",
        "WRITE_CALENDAR"
      ]
    },
    "web": {
      "favicon": "./assets/profile-pic.png"
    }
  }
}
